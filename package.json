{
  "name": "multiview",
  "version": "3.0.1",
  "description": "Neatly view the output of multiple spawned processes.",
  "main": "main.js",
  "scripts": {
    "test-eslint": "eslint ./ --ext js",
    "test": "npm run test-eslint && nodeunit test/test.js && node cli [ls -la] [npm run lsla] -x 5000",
    "lsla": "ls -la"
  },
  "bin": {
    "multiview": "cli.js"
  },
  "repository": {
